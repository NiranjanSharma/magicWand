<html lang="en">

  <head>
    <link rel="stylesheet" type="text/css" href="css/template.css" />
  </head>

  <body class="container">

    <header>
      <%include ../partials/header %>
    </header>

    <head>
      <h2><% include ../partials/head %>Template</h2>
    </head>

    <form action="/prepareObject/generateJson" method="post" enctype = "multipart/form-data">

      <div class="col-xs-4">
        <!-- Charecter Name -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Enter your Character Name</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="characterName" 
            type="text" 
            class="form-control" 
            name="characterName" 
            minlength="1"
            maxlength="15"
            placeholder="Enter your Character Name"
            required>
          </div>
        </div>
        <!-- Charecter Profession -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Enter your Character profession</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="characterProfession" 
            type="text" 
            class="form-control" 
            name="characterProfession" 
            minlength="1"
            maxlength="20"
            placeholder="Enter your Character profession" 
            required>
          </div>
        </div>
        <!-- Problem Statement -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Enter the problem statement</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="problemStatement" 
            type="text" 
            class="form-control" 
            name="problemStatement" 
            minlength="1"
            maxlength="70"
            placeholder="Enter the problem customer is facing with" 
            required>
          </div>
        </div>
        <!-- First Option -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Enter the first option explored</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="exploredOption1" 
            type="text" 
            class="form-control" 
            name="exploredOption1" 
            minlength="1"
            maxlength="15"
            placeholder="Enter the first option he explored" 
            required>
          </div>
        </div>
        <!-- Second Option -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Enter the second option explored</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="exploredOption2" 
            type="text" 
            class="form-control" 
            name="exploredOption2" 
            minlength="1"
            maxlength="15"
            placeholder="Enter the second option he explored" 
            required>
          </div>
        </div>
        <!-- Third Option -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Enter the third option explored</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="exploredOption3" 
            type="text" 
            class="form-control" 
            name="exploredOption3" 
            minlength="1"
            maxlength="15"
            placeholder="Enter the third option he explored" 
            required>
          </div>
        </div>
        <!-- Product Name -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Enter your product name</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="productName" 
            type="text" 
            class="form-control" 
            name="productName" 
            minlength="1"
            maxlength="20"
            placeholder="Enter your product name" 
            required>
          </div>
        </div>
        <!-- Product Logo -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Upload your Product Logo</label>
          </div>

          <div class="col-xs-12">
            <input 
            type="file"
            accept=".png, .jpg, .jpeg" 
            class="btn btn-default btn-file col-xs-12" 
            name="productLogo" required >
          </div>
        </div>
      </div>

      <div class="col-xs-4">
        <!-- Product Slogan -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Enter Product Slogan</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="slogan" 
            type="text" 
            class="form-control" 
            name="slogan" 
            placeholder="Enter Product Slogan"
            minlength="1"
            maxlength="70"
            required>
          </div>
        </div>
        <!-- First Benefit -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Provide your first benefit</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="benefit1" 
            type="text" 
            class="form-control" 
            name="benefit1" 
            placeholder="Provide your first benefit"
            minlength="1"
            maxlength="70"
            required>
          </div>
        </div>
        <!-- Second Benefit -->
        <div class="col-xs-12">
          <div class="col-xs-12">
           <label class="lblForm">Provide your second benefit</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="benefit2" 
            type="text" 
            class="form-control" 
            name="benefit2" 
            placeholder="Provide your second benefit"
            minlength="1"
            maxlength="70"
            required>
          </div>
        </div>
        <!-- Third Benefit -->
        <div class="col-xs-12">
          <div class="col-xs-12">
           <label class="lblForm">Provide your third benefit</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="benefit3" 
            type="text" 
            class="form-control" 
            name="benefit3" 
            placeholder="Provide your third benefit"
            minlength="1"
            maxlength="70"
            required>
          </div>
        </div>
        <!-- Solution Offered -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Provide the solution offered</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="soultionOffered" 
            type="text" 
            class="form-control" 
            name="soultionOffered" 
            placeholder="Enter solution  of Product"
            minlength="1"
            maxlength="15"
            required>
          </div>
        </div>
        <!-- First Key Feature -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Provide the first key feature</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="keyFeature1" 
            type="text" 
            class="form-control" 
            name="keyFeature1" 
            placeholder="Provide the first key feature"
            minlength="1"
            maxlength="70"
            required>
          </div>
        </div>
        <!-- Second Key Feature -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Provide the second key feature</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="keyFeature2" 
            type="text" 
            class="form-control" 
            name="keyFeature2" 
            placeholder="Provide second key feature"
            minlength="1"
            maxlength="70"
            required>
          </div>
        </div>
        <!-- Third Key Feature -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Provide the third key feature</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="keyFeature3" 
            type="text" 
            class="form-control" 
            name="keyFeature3" 
            placeholder="Provide the second key feature"
            minlength="1"
            maxlength="70"
            required>
          </div>
        </div>
      </div>

      <div class="col-xs-4">
        <!-- First Screen -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Upload your first screen</label>
          </div>

          <div class="col-xs-12">
            <input 
            type="file"
            accept=".png, .jpg, .jpeg" 
            class="btn btn-default btn-file col-xs-12" 
            name="screen1" required >
          </div>
        </div>
        <!-- First Screen Text -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Provide the Screen1 text</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="screen1Text" 
            type="text" 
            class="form-control" 
            name="screen1Text" 
            placeholder="Provide the Screen1 text"
            minlength="1"
            maxlength="50"
            required>
          </div>
        </div>
        <!-- Second Screen -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Upload your second screen</label>
          </div>

          <div class="col-xs-12">
            <input 
            type="file" 
            accept=".png, .jpg, .jpeg" 
            class="btn btn-default btn-file col-xs-12" 
            name="screen2" required >
          </div>
        </div>
        <!-- Second Screen Text -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Provide the Screen2 text</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="screen2Text" 
            type="text" 
            class="form-control" 
            name="screen2Text" 
            placeholder="Provide the Screen2 text"
            minlength="1"
            maxlength="50"
            required>
          </div>
        </div>
        <!-- Third Screen -->
        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Upload your third screen</label>
          </div>

          <div class="col-xs-12">
            <input 
            type="file" 
            accept=".png, .jpg, .jpeg" 
            class="btn btn-default btn-file col-xs-12" 
            name="screen3" required >
          </div>
        </div>

        <div class="col-xs-12">
          <div class="col-xs-12">
            <label class="lblForm">Provide the Screen3 text</label>
          </div>

          <div class="col-xs-12">
            <input 
            id="screen3Text" 
            type="text" 
            class="form-control" 
            name="screen3Text" 
            placeholder="Provide the Screen3 text"
            minlength="1"
            maxlength="50"
            required>
          </div>
        </div>

        <div class="col-xs-12">
          <div class="col-xs-12">
           <label class="lblForm">Entery your name</label>
         </div>

         <div class="col-xs-12">
          <input 
          id="output" 
          type="text" 
          class="form-control" 
          name="output" 
          placeholder="Enter your name"
          minlength="1"
          maxlength="15"
          required>
        </div>
      </div>

  <div class="col-xs-12">
    <div class="col-xs-12">
      <label class="lblForm">Website</label>
    </div>

    <div class="col-xs-12">
      <input 
      id="website" 
      type="text" 
      class="form-control" 
      name="website" 
      placeholder="Enter a Website URL"
      minlength="1"
      maxlength="25"
      required> 
      <!-- pattern="http://stackoverflow.com"www.?-?(\w+)*.[A-Za-z0-9].com   -->
    </div>
  </div>

  <div class="col-xs-12">
    <div class="col-xs-12">
      <button type="submit" id="btnlogin" class="btn btn-default btn-lg" style="margin-top: 10px">Generate</button>
    </div>
  </div>
  </div>
  </form>
  </body>
  </html>

